# BDAP Server Configuration
# Copy this file to .env and fill in your values

# BDAP server hostname
BDAP_HOST=

# BDAP server SSH port (default: 22)
BDAP_PORT=22

# BDAP server SSH username
BDAP_USERNAME=

# BDAP server SSH password
BDAP_PASSWORD=

# Remote directory on BDAP server for file storage
BDAP_REMOTE_DIR= 